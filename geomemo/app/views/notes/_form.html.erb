<%= form_for @note do |f| %>
  <div>
    <%= f.label :title %>
    <br>
    <%= f.text_field :title %>
  </div>
  <br>
  <div>
    <%= f.label :content %>
    <br>
    <%= f.text_area :content %>
  </div>
  <br>
  <div>
    <%= f.label :notebook %>
    <br>
    <%= f.collection_select :notebook_id, @notebook, :id, :name  %>
  </div>
  <br>
  <div>
    <%= f.label :location %>
    <br>
    <%= f.collection_select :location_id, @location, :id, :name  %>
  </div>

  <%= f.submit %>

<% end %>
