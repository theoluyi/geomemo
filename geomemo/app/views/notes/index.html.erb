<h1> All Notes </h1>
<%= button_to "New Note", new_note_path, method: "get" %>
<% @notes.each do |note| %>
  <ul>
    <li>
      <%= link_to note.title, note_path(note) %> <%= button_to "Edit", edit_note_path(note), method: :get %>
      <%= button_to "Delete", note_path(note), method: "delete" %>
    </li>
  </ul>
<% end %>
